filebeat.prospectors:

- input_type: log

  paths:
        - /app/storage/logs/*.log

  document_type: {{ app_name }}

  multiline:
    pattern: '^\[[0-9]{4}-[0-9]{2}-[0-9]{2}'
    negate: true
    match: after

tags: [{{ app_name }}]

fields:
  env: {{ app_env }}

output.logstash:
  hosts: ["incoming.logs.digitalpfizer.com:6799"]

  ssl.certificate_authorities: ["/etc/ssl/entrust_ev_ca.cer"]